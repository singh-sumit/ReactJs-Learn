    }, 1000)\r\n    );\r\n  }\r\n\r\n  useEffect(() => {\r\n    clearInterval(timer);\r\n    playing? audio.play() : audio.pause();\r\n\r\n    console.log(\"called\");\r\n  }, [isAlert,playing]);\r\n\r\n  function onCloseDialog() {\r\n    setOpenDialog(false);\r\n    setPlaying(false);\r\n  }\r\n  return (\r\n    <div style={{ maxWidth: 600, margin: \"auto\", textAlign: \"center\" }}>\r\n      <h1>This is Timer App.</h1>\r\n      <Grid container justify=\"flex-start\" spacing={2}>\r\n        <Grid item xs=\"9\" sm=\"9\">\r\n          <TextField\r\n            id=\"time\"\r\n            onChange={handleChange}\r\n            value={time}\r\n            label=\"Please Select Your Time\"\r\n            helperText=\"Set timer in the basis of seconds\"\r\n            fullWidth={true}\r\n            type=\"number\"\r\n            variant=\"filled\"\r\n          />\r\n        </Grid>\r\n        <Grid item xs=\"3\" sm=\"3\">\r\n          <Button onClick={runTimer}>Start Timer</Button>\r\n        </Grid>\r\n      </Grid>\r\n      <div>\r\n        <Card>\r\n          <h2>{auxCount}</h2>\r\n        </Card>\r\n        <CustomDialog\r\n          openDialouge={openDialog}\r\n          onCloseDialog={onCloseDialog}\r\n          dialogContent={<div>Alert! Alert! It's time Now</div>}\r\n        />\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\sumit\\Documents\\ReactJs-Learn\\react-app-jmbo\\src\\week-3\\timer\\customDialog.js",[],"C:\\Users\\sumit\\Documents\\ReactJs-Learn\\react-app-jmbo\\src\\week-3\\react-hook\\api-call-example.js",["74","75","76"],{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","severity":1,"message":"82","line":1,"column":8,"nodeType":"83","messageId":"84","endLine":1,"endColumn":12},{"ruleId":"81","severity":1,"message":"85","line":6,"column":3,"nodeType":"83","messageId":"84","endLine":6,"endColumn":7},{"ruleId":"86","severity":1,"message":"87","line":39,"column":9,"nodeType":"88","endLine":43,"endColumn":10},{