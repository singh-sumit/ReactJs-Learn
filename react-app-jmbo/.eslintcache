\r\n          <TextField\r\n            id=\"outlined-basic\"\r\n            id=\"email\"\r\n            label=\"Email\"\r\n            variant=\"outlined\"\r\n            placeholder=\"Enter email\"\r\n            helperText=\"Please enter valid email\"\r\n            error={false}\r\n            disabled={false}\r\n            required={true}\r\n            fullWidth={true}\r\n            value={userProfile.email}\r\n            onChange={handleChange}\r\n          />\r\n        </Grid>\r\n        <Grid item xs=\"12\" sm=\"4\">\r\n          <TextField\r\n            id=\"outlined-basic\"\r\n            id=\"address\"\r\n            label=\"Address\"\r\n            variant=\"outlined\"\r\n            placeholder=\"Enter Address\"\r\n            helperText=\"Please enter valid address\"\r\n            error={false}\r\n            disabled={false}\r\n            required={true}\r\n            fullWidth={true}\r\n            value={userProfile.address}\r\n            onChange={handleChange}\r\n          />\r\n        </Grid>\r\n      </Grid>\r\n\r\n      <Card>\r\n          <Grid container spacing={2}>\r\n              <Grid item xs=\"6\" sm=\"6\">Name :</Grid>\r\n              <Grid item xs=\"6\" sm=\"6\">{userProfile.name}</Grid>\r\n          </Grid>\r\n          <Grid container spacing={2}>\r\n              <Grid item xs=\"6\" sm=\"6\">Address :</Grid>\r\n              <Grid item xs=\"6\" sm=\"6\">{userProfile.address}</Grid>\r\n          </Grid>\r\n          <Grid container spacing={2}>\r\n                <Grid item xs=\"6\" sm=\"6\">Email :</Grid>\r\n                <Grid item xs=\"6\" sm=\"6\">{userProfile.email}</Grid>\r\n          </Grid>\r\n      </Card>\r\n      {/* <p>{userProfile.name}</p>\r\n      <p>{userProfile.email}</p> */}\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\sumit\\Documents\\ReactJs-Learn\\react-app-jmbo\\src\\week-3\\react-hook\\effectHook.js",[],"C:\\Users\\sumit\\Documents\\ReactJs-Learn\\react-app-jmbo\\src\\week-3\\timer\\timerApp.js",[