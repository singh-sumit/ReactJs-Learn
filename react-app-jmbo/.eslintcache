{() => handleLight()}\r\n        ></img>\r\n        {/* sending image as props to second hook */}\r\n        <MySecondHook\r\n          imageLoc={imgUrl}\r\n          name={\"This is me Sumit\"}\r\n          localImage={Bulb}\r\n        />\r\n        {/* using usehistory to route to certian path */}\r\n        <button onClick={() => history.push(\"inc\")}>Click to go</button>\r\n        <button onClick={() => history.push(\"third/profile/dfsd\",{name: 'Sumit'})}>Click to go third</button>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default MyFirstHook;\r\n","C:\\Users\\sumit\\Documents\\ReactJs-Learn\\react-app-jmbo\\src\\week-3\\react-hook\\prevState.js",[],"C:\\Users\\sumit\\Documents\\ReactJs-Learn\\react-app-jmbo\\src\\week-3\\react-hook\\secondReactHook.js",["48","49"],"import React from 'react';\r\n\r\nexport default function MySecondHook(props){\r\n    console.log(props.imageLoc);\r\n    return (\r\n        <div>\r\n            This is second Component.\r\n            <img src={props.imageLoc} style={{width: '30%'}}></img>\r\n            <p>{props.name}</p>\r\n            <img src={props.localImage}></img>\r\n        </div>\r\n    )\r\n}","C:\\Users\\sumit\\Documents\\ReactJs-Learn\\react-app-jmbo\\src\\week-3\\react-hook\\thirdReactHook.js",[],"C:\\Users\\sumit\\Documents\\ReactJs-Learn\\react-app-jmbo\\src\\week-3\\react-hook\\userProfile.js",["50","51","52","53"],{"ruleId":"54","replacedBy":"55"},{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","severity":1,"message":"59","line":1,"column":8,"nodeType":"60","messageId":"61","endLine":1,"endColumn":12},{"ruleId":"58","severity":1,"message":"62","line":6,"column":3,"nodeType":"60","messageId":"61","endLine":6,"endColumn":7},{"ruleId":"63","severity":1,"message":"64","line":39,"column":9,"nodeType":"65","endLine":43,"endColumn":10},{"ruleId":"63","severity":1,"message":"64","line":8,"column":13,"nodeType":"65","endLine":8,"endColumn":62},{"ruleId":"63","severity":1,"message":"64","line":10,"colum